use P5HPBP1
declare @date date, @enddate date;

set @date = '20220716';
set @enddate = '20220719';

SELECT POSTINGNO, ACCOUNTNO, CDA.PAYEREMAIL, convert(date,POS.DOCUMENTDATE) as DOCUMENT_DATE
FROM ACC_POSTING as POS,
	LHR_CONTACTDATA as CDA

WHERE POS.EXTERNALCOMPANYCODE = '2'
	AND POS.ACCOUNTCATEGORYKEY = 0
	AND convert(date, CREATETIME ) between @date and @enddate
	AND POS.ACCOUNTCODE = CDA.PAYERCODE
ORDER BY CDA.PAYERCODE
